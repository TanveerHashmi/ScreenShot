@model List<MyApplication.Models.DogViewModel>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}


<div class="screencontent">

<h1>Hi this is Index Page</h1>

@foreach (var dog in Model)
{
    <li>
        <h4>A dog whose name is @dog.Name and Age @dog.Age Years old with @Html.Raw(dog.Description) in Color @*@dog.Description*@ </h4>

    </li>
}

</div>


@section Scripts
{
    <script src="~/lib/tinymce/tinymce.js"></script>
    <script src="~/lib/tinymce/tinymce.min.js"></script>
    <script src="~/lib/jquery/dist/main.js"></script>
}

